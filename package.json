{
  "name": "cheapExp",
  "version": "1.0.0",
  "description": "Explorer plus plus for archive",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --require babel-register",
    "buildDev": "webpack --progress --mode development",
    "buildProd": "webpack --progress --mode production",
    "devServer": "webpack-dev-server --mode development",
    "watchBuildDev": "webpack --watch --progress --mode development",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "windows": "electron-builder --windows portable --x64",
    "mac": "electron-builder --mac dmg",
    "electron": "electron ./public/electron.js",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "document-wip": "jsdoc2md ./src/*.js > ./docs/doc.md"
  },
  "build": {
    "appId": "io.github.archifiltre",
    "files": [
      "electron/index.js",
      "electron/dist",
      "electron/build/icon.*",
      "package.json"
    ],
    "mac": {
      "icon": "./electron/build/icon.icns"
    }
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeanbaptisteassouad/cheapExp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeanbaptisteassouad/cheapExp/issues"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-react": "^0.6.0",
    "electron-builder": "^20.44.4",
    "react-app-rewired": "^2.1.3",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.4.0",
    "dateformat": "3.0.3",
    "electron": "^5.0.5",
    "electron-ga": "1.0.6",
    "electron-is-dev": "^1.1.0",
    "file-saver": "2.0.2",
    "foundation-sites": "^6.5.3",
    "immutable": "4.0.0-rc.9",
    "jquery": "^3.4.1",
    "js-sha512": "0.8.0",
    "jszip": "3.1.5",
    "node-sass": "^4.12.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-scripts": "^3.0.1",
    "riek": "1.1.0",
    "tar-fs": "1.16.3",
    "tar-stream": "1.6.2",
    "what-input": "^5.2.1",
    "xml": "1.0.1"
  }
}
